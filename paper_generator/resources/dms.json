rator </span><span style=\"color:#0000ff;background-color:#efefef;font-weight:bold;\">key=</span><span style=\"color:#008000;background-color:#efefef;font-weight:bold;\">'[ \"ccm.get\", \"resources/configs.js\", \"small\" ]' </span><span style=\"color:#0000ff;background-color:#efefef;font-weight:bold;\">process_all_results=</span><span style=\"color:#008000;background-color:#efefef;font-weight:bold;\">\"my_plots\"</span><span style=\"background-color:#efefef;\">&gt;</span><br> <span style=\"background-color:#efefef;\"> &lt; </span><span style=\"color:#000080;background-color:#efefef;font-weight:bold;\">div </span><span style=\"color:#0000ff;background-color:#efefef;font-weight:bold;\">id=</span><span style=\"color:#008000;background-color:#efefef;font-weight:bold;\">\"paper\"</span><span style=\"background-color:#efefef;\">&gt;</span><br> <span style=\"background-color:#efefef;\"> &lt; </span><span style=\"color:#000080;background-color:#efefef;font-weight:bold;\">header </span><span style=\"color:#0000ff;background-color:#efefef;font-weight:bold;\">class=</span><span style=\"color:#008000;background-color:#efefef;font-weight:bold;\">\"paper\"</span><span style=\"background-color:#efefef;\">&gt; &lt; /</span><span style=\"color:#000080;background-color:#efefef;font-weight:bold;\">header</span><span style=\"background-color:#efefef;\">&gt;</span><br> <span style=\"background-color:#efefef;\"> &lt; </span><span style=\"color:#000080;background-color:#efefef;font-weight:bold;\">article </span><span style=\"color:#0000ff;background-color:#efefef;font-weight:bold;\">id=</span><span style=\"color:#008000;background-color:#efefef;font-weight:bold;\">\"main\"</span><span style=\"background-color:#efefef;\">&gt;</span><br> <span style=\"background-color:#efefef;\"> &lt; </span><span style=\"color:#000080;background-color:#efefef;font-weight:bold;\">section </span><span style=\"color:#0000ff;background-color:#efefef;font-weight:bold;\">class=</span><span style=\"color:#008000